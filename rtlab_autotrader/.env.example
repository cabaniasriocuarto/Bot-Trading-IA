RTLAB_CONFIG_PATH=rtlab_config.yaml
RTLAB_USER_DATA_DIR=
LOG_LEVEL=INFO
PORT=8080

# Auth (server-side only)
AUTH_SECRET=replace-with-32-plus-random-chars
ADMIN_USERNAME=Wadmin
ADMIN_PASSWORD=change-me-admin-password
VIEWER_USERNAME=viewer
VIEWER_PASSWORD=change-me-viewer-password
INTERNAL_PROXY_TOKEN=replace-with-shared-bff-token-min-32-chars

# Runtime mode
MODE=paper
EXCHANGE_NAME=binance
BOTS_MAX_INSTANCES=30
BOTS_OVERVIEW_CACHE_TTL_SEC=10
BOTS_OVERVIEW_INCLUDE_RECENT_LOGS=true
BOTS_OVERVIEW_PROFILE_SLOW_MS=500
BOTS_OVERVIEW_SLOW_LOG_THROTTLE_SEC=30

# Exchange credentials (set real values in Railway, never commit secrets)
# Principio de mínimo privilegio: Read + Trade únicamente. NUNCA Withdraw.
# Preferred names:
BINANCE_TESTNET_API_KEY=
BINANCE_TESTNET_API_SECRET=
BINANCE_SPOT_TESTNET_BASE_URL=https://testnet.binance.vision
BINANCE_SPOT_TESTNET_WS_URL=wss://testnet.binance.vision/ws
BINANCE_API_KEY=
BINANCE_API_SECRET=
BINANCE_SPOT_BASE_URL=https://api.binance.com
BINANCE_SPOT_WS_URL=wss://stream.binance.com:9443/ws

# Legacy compatibility (optional)
API_KEY=
API_SECRET=
TESTNET_API_KEY=
TESTNET_API_SECRET=

# Telegram alerts
TELEGRAM_ENABLED=false
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# Research / observabilidad (opcionales)
MLFLOW_TRACKING_URI=
GIT_COMMIT=
